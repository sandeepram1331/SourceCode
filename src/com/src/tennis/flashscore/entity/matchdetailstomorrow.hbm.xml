<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.src.tennis.flashscore.entity.MatchDetailsTomorrow"
		table="match_details_tomorrow">
		<id name="matchId" type="string">
			<column name="matchId" />
		</id>
		<property name="player1" type="string">
			<column name="player1" />
		</property>
		<property name="player2" type="string">
			<column name="player2" />
		</property>
		<property name="date" type="timestamp">
			<column name="date" />
		</property>
		
		
	</class>
</hibernate-mapping>	